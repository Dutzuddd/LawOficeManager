@model IEnumerable<LicentaSfranciog.Models.Raport>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumeProces)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OreLucrate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Facturat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cheltuieli)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataRaport)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Proces)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumeProces)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OreLucrate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Facturat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cheltuieli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataRaport)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Proces.Nume)
                </td>
            <td>
               
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
